@model IEnumerable<GarageDatabse.Models.Vehicle>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Owner.PersonalNumber)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ParkingSpot.ID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.VehicleType.ID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RegNumber)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Owner.PersonalNumber)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ParkingSpot.ID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.VehicleType.ID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RegNumber)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID })
        </td>
    </tr>
}

</table>
